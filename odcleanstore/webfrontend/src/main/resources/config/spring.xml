<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans"    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    xmlns:context="http://www.springframework.org/schema/context"    xsi:schemaLocation="http://www.springframework.org/schema/beans                         http://www.springframework.org/schema/beans/spring-beans.xsd                        http://www.springframework.org/schema/context                         http://www.springframework.org/schema/context/spring-context.xsd">	<!-- Properties -->	<context:property-placeholder location="./config/*.properties" />		<!-- Application Configuration -->	<bean 		id="cleanConnectionCoords" 		class="cz.cuni.mff.odcleanstore.connection.JDBCConnectionCredentials">				<constructor-arg value="${jdbc.clean.url}" />		<constructor-arg value="${jdbc.clean.username}" />		<constructor-arg value="${jdbc.clean.password}" />	</bean>		<bean 		id="dirtyConnectionCoords" 		class="cz.cuni.mff.odcleanstore.connection.JDBCConnectionCredentials">				<constructor-arg value="${jdbc.dirty.url}" />		<constructor-arg value="${jdbc.dirty.username}" />		<constructor-arg value="${jdbc.dirty.password}" />	</bean>		<bean		id="appConfig"		class="cz.cuni.mff.odcleanstore.webfrontend.configuration.Configuration">				<constructor-arg ref="cleanConnectionCoords" />		<constructor-arg ref="dirtyConnectionCoords" />		<constructor-arg value="${gmail.address}" />		<constructor-arg value="${gmail.password}" />	</bean></beans>